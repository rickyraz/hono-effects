{
  "name": "hono-effectts",
  "version": "1.0.0",
  "description": "Hono backend framework integrated with Effect-TS following best practices",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "echo 'Linting skipped - TypeScript compilation provides type checking'",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.6.3",
    "effect": "^3.10.0",
    "@effect/platform": "^0.66.0",
    "@effect/platform-node": "^0.63.0",
    "@effect/schema": "^0.75.0",
    "@hono/node-server": "^1.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "@vitest/coverage-v8": "^2.1.3",
    "eslint": "^9.14.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0"
  },
  "keywords": [
    "hono",
    "effect-ts",
    "backend",
    "typescript",
    "functional-programming"
  ],
  "author": "",
  "license": "MIT"
}